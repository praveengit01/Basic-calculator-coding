print('''
+ ADD
- SUBTRACT
* MULTIPLY
/ DIVIDE
''')
num1=int(input("Enter The Value1:-"))
num2=int(input("Enter The Value2:-"))
opr=input("Enter The Opr..(+,_,*,/)")
if opr=="+":
    print(num1+num2)
elif opr=="-":
    print(num1-num2)
elif opr=="*":
    print(num1*num2)
elif opr=="/":
    print(num1/num2)
else:
    print("invalid opr...")
